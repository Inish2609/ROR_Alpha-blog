<h1> Edit User </h1>
<%= form_with model:@user,method: :patch,local:true do |f| %>
    <p>
        <%= f.label :username %>
        <%= f.text_field :username %>
    </p>
    <p>
        <%= f.label :email %>
        <%= f.email_field :email %>
    </p>
    <p>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </p>
    <p>
        <%= f.file_field :avatar %>
    </p>
    <p>
        <%= f.submit "Update User" %>
    </p>
<% end %>